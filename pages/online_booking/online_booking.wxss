/* pages/online_booking/online_booking.wxss */
.header {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 80px;
    z-index: 111111;
    background: #fff;
}
.goBack {
    left: 20px;
    top: 20px;
    width: 20px;
    height: 20px;
}
.header .h2 {
    height: 60px;
    line-height: 60px;
    text-align: center;
    font-weight: normal;
    font-size: 20px;
    /*background: #fff url(../images/goback.png) no-repeat 20px center;*/
    background-size: 20px;
}
.header .more {
    /* background: url(../images/music.png) no-repeat center center; */
    background-size: 20px;
    margin-left: -1px;
    display: block;
    width: 20px;
    height: 20px;
    right: 24px;
    top: 20px;
}
.serchNav {
    width: 100%;
    height: 130px;
    background: linear-gradient(90deg,rgba(77,183,254,1),rgba(0,153,255,1));
}
.ontop {
    height: 66px;
}
.serchNav view.fl {
    width: 33%;
    color: #fff;
    height: 100%;
}
.serchNav view.fl view {
    font-size: 16px;
    text-align: left;
}
.qsBox view, .mdBox view {
    overflow: hidden;
    text-overflow: ellipsis;
    -o-text-overflow: ellipsis;
    white-space: nowrap;
}
.imgarow{
  display: block;
  width:80%;
  height:10px;
  margin:30px auto 0;
}
.serchNavOl view.fl .tgp {
    text-align: center;
    margin-top: 5px;
    width:100%;
}
.serchNavOl .qsBox, .serchNavOl .mdBox {
    padding-top: 20px;
}
.qsBox, .mdBox {
    padding-top: 30px;
    box-sizing: border-box;
    padding-left: 10px;
}
.shipDetail {
    box-sizing: border-box;
    padding: 10px;
}
.shipDetail view.fl view {
    font-size: 14px;
    color: #fff;
}
.shipDetail .fr {
    font-size: 14px;
    color: #fff;
}
.navtitle {
    background: #fff;
    height: 34px;
    line-height: 34px;
    border-bottom: 1px solid #ebebeb;
    color: #666;
    font-size: 14px;
    text-align: center;
}